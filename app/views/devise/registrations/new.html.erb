<div class="card">
  <div class="card-header">
    <h2>Sign up</h2>
  </div>
  <div class="card-body">
    <p class="card-text">
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="field form-group">
    <br />
    <%= f.email_field :email, autofocus: false, autocomplete: "email", class:"form-control", placeholder:"Email" %>
  </div>

 <div class="field form-group"> <br /> <%= f.password_field :password,
 autocomplete: "new-password", class: "form-control", placeholder:"Password (6
 Characters Minimum)" %> </div>

 <div class="field form-group">
   <br />
   <%= f.password_field :password_confirmation, autocomplete: "new-password", class:"form-control", placeholder:"Confirm Password" %>
 </div>

  <div class="actions">
    <br/>
    <%= f.submit "Sign up", class: "btn btn-dark" %>
  </div>

    </p>
  </div>
  <% end %> 
  <div class="card">
    <div class="card-body">
  <%= render "devise/shared/links" %>
</div>
  </div>
</div>